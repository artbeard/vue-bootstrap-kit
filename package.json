    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@storybook/vue3": "^7.4.5",
    "@storybook/vue3-vite": "^7.4.5",
    "@tabler/core": "^1.0.0-beta20",
    "@tabler/icons-webfont": "^2.11.0",
    "@types/node": "^20.6.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.68.0",
    "storybook": "^7.4.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.5"
  },
  "dependencies": {
    "flatpickr": "^4.6.13",
    "jodit": "^4.0.0-beta.61",
    "slim-select": "^2.6.0"
  }
